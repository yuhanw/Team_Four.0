data_clean=function(i){
df=read.csv(paste('../data1/raw/',i,'_to_be_cleaned.csv',sep=''), header=T)
df=subset(df, select=c(grp_code, state, group, emp_rate, unemp_rate))
df=df[complete.cases(df),]
dfgen=subset(df,grp_code==c(2,3)) 
dfage=subset(df,grp_code==c(26:39))
dfrace=subset(df,grp_code==c(5,6,8,9,11,12,14,15))
return (list(df,dfgen, dfage, dfrace))}
                     


